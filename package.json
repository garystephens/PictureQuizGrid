{
    "name": "picture-quiz-grid-generator",
    "version": "1.0.0",
    "description": "Generate picture quiz numbered grids simply by uploading images.",
    "main": "main.js",
    "scripts": {
        "dev": "rmdir /s /q dist && webpack --mode development",
